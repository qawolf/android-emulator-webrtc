{
  "name": "android-emulator-webrtc",
  "version": "1.0.18",
  "description": "Android Emulator WebRTC module",
  "scripts": {
    "build": "tsc && babel src -d dist --extensions \\\".ts,.tsx\\\"",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "/node_modules/@juggle/resize-observer/"
    ]
  },
  "dependencies": {
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.24.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-polyfill": "^6.26.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.2.0",
    "react-docgen-markdown-renderer": "^2.1.3",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "typescript": "5.5.3",
    "yalc": "^1.0.0-pre.53"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/google/android-emulator-webrtc.git"
  },
  "keywords": [
    "webrtc",
    "android",
    "emulator"
  ],
  "author": "Erwin Jansen <pokowaka@gmail.com> (https://github.com/pokowaka/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/google/android-emulator-container-scripts/issues"
  },
  "homepage": "https://github.com/google/android-emulator-container-scripts#js/android-emulator-webrtc/README.MD"
}
